//Given celsius
let celsius = 99;
console.log('Given Celsius: '+celsius+'C')
//Converts celsius to Fahrenheit
function convertsCTF(){
    //Formula for converting Celsius to Fahrenheit
    let Fahrenheit = (celsius * 9/5)+32;
    console.log('Converts given Celsius to Fahrenheit Temperature is '+Fahrenheit+'F');
}
//Calling function
convertsCTF();